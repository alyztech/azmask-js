{
  "name": "azmask-js",
  "version": "0.1.0",
  "repository": "https://github.com/alyztech/azmask-js",
  "author": "Caio Mansho",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "docs": "typedoc src/index.ts --out docs --listInvalidSymbolLinks --includes tutorials --theme pages-plugin --includeVersion",
    "compile": "yarn compile:esm && yarn compile:cjs",
    "compile:esm": "rm -rf esm && tsc && node ./build-scripts/compile.esm.after.mjs",
    "compile:cjs": "rm -rf dist && tsc --project tsconfig.cjs.json && node ./build-scripts/compile.cjs.after.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx,.mjs",
    "lint:fix": "yarn lint --fix",
    "prepack": "yarn compile",
    "prepublishOnly": "yarn audit && yarn lint && yarn test && yarn docs",
    "start:dev": "nodemon",
    "start:dev:compatibility": "TS_NODE_FILES=true yarn start:dev",
    "testDrop": "jest --coverage src/drop",
    "testMaskFactory": "jest --coverage src/maskFactory",
    "testTextCache": "jest --coverage src/textCache",
    "testAzMask": "jest --coverage src/azMask",
    "testAzMaskGroup": "jest --coverage src/azMaskGroup",
    "testValidateRegex": "jest --coverage src/validateRegex"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./esm/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./esm/index.d.ts",
  "module": "./esm/index.js",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@knodes/typedoc-plugin-pages": "^0.23.1",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-plugin-module-extension": "^0.1.3",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "fs-extra": "^9.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "must": "^0.13.4",
    "nodemon": "^2.0.19",
    "ts-jest": "^27.1.5",
    "ts-node": "^9.0.0",
    "typedoc": "^0.23.8",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepublishOnly && yarn compile"
    }
  },
  "libraryTemplate": {
    "version": "3.5.1",
    "language": "typescript",
    "fixDefaultForCommonJS": true,
    "jsx": false
  }
}
